
# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:arrombasse.com.br
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} ^arrombasse.com.br$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
